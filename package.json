{
  "name": "grammar_review_tool",
  "version": "0.0.0",
  "description": "Tool for review formal grammars in Lemon format.",
  "private": true,
  "scripts": {
    "build": "node resources/assets/build/build.js",
    "dev": "node resources/assets/build/dev-server.js",
    "test": "npm run lint && npm run unit",
    "unit": "karma start resources/assets/test/unit/karma.conf.js --single-run",
    "report-coverage": "cat ./resources/assets/test/unit/coverage/lcov.info | node_modules/codecov.io/bin/codecov.io.js",
    "lint": "eslint --ext .js,.vue .",
    "semantic-release": "semantic-release pre && semantic-release post",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/melihovv/grammar_review_tool.git"
  },
  "keywords": [
    "grammar",
    "lemon",
    "review"
  ],
  "author": "Alexander Melihov <amelihovv@ya.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/melihovv/grammar_review_tool/issues"
  },
  "homepage": "https://github.com/melihovv/grammar_review_tool",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.8.2",
    "compression-webpack-plugin": "^0.3.1",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.27.3",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "eventsource-polyfill": "^0.9.6",
    "expose-loader": "^0.7.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.0",
    "husky": "^0.12.0",
    "inject-loader": "^3.0.0-beta4",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-sinon-chai": "^1.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.3",
    "lolex": "^1.5.1",
    "mocha": "^2.5.3",
    "raw-loader": "^0.5.1",
    "semantic-release": "^4.3.5",
    "shelljs": "^0.6.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.14.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "validate-commit-msg": "^2.6.1",
    "vue-hot-reload-api": "^2.0.8",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.1.4",
    "vue-style-loader": "^2.0.4",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "antlr4": "^4.6.0",
    "babel-runtime": "^6.11.6",
    "bootstrap": "^3.3.7",
    "brace": "^0.9.1",
    "debounce": "^1.0.0",
    "jquery": "^3.1.0",
    "messenger": "HubSpot/messenger",
    "vue": "^2.2.4",
    "vue-multiselect": "next"
  }
}
